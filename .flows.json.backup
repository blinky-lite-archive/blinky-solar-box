        "name": "script.updateReadings",
        "field": "payload.script.updateReadings",
        "fieldType": "msg",
        "format": "javascript",
        "syntax": "mustache",
        "template": "// Update Readings Javascript\n        function updateDmaReadings()\n        {\n            $('#linkQualityId').text(device.linkQuality.value);\n            $('#signalLevelId').text(device.signalLevel.value);\n            var power = device.power.value;\n            var relay = device.relay.value;\n            powerStripChart.addTracePoints(power, relay);\n            $('#powerId').text(power);\n            var buttonColor = '#00ff00';\n            var buttonTextColor = '#000000';\n            var buttonText = \"Off\";\n            if (relay > 0) \n            {\n                buttonColor = '#ff0000';\n                buttonText = 'On';\n            }\n            $('#relayButtonId').css('color', buttonTextColor); \n            $('#relayButtonId').css('background-color', buttonColor); \n            $('#relayButtonId').text(buttonText); \n        }\n",
        "output": "str",
